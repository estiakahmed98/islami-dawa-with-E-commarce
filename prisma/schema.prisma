model AmoliMuhasabaData {
  id          Int    @id @default(autoincrement())
  tahajjud    String
  ayat        String
  zikir       String
  ishraq      String
  jamat       String
  sirat       String
  Dua         String
  ilm         String
  tasbih      String
  dayeeAmol   String
  amoliSura   String
